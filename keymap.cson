'.platform-win32, .platform-linux':
  'ctrl-k ctrl-b': 'unset!'
  'alt-t':'tree-view:toggle'
  'ctrl-shift-t':'tree-view:reveal-active-file'

'.platform-darwin':
  'cmd-k cmd-b': 'unset!'
  'cmd-t':'tree-view:toggle'
  'ctrl-shift-t':'tree-view:reveal-active-file'
  'ctrl-t': 'fuzzy-finder:toggle-file-finder'

'.platform-win32 .tree-view, .platform-linux .tree-view':
# ====== UNDO ALL THE CTRL-K STUFF ======
  'ctrl-k up': 'unset!'
  'ctrl-k right': 'unset!'
  'ctrl-k left': 'unset!'
  'ctrl-k down': 'unset!'
  'ctrl-k l': 'unset!'
  'ctrl-k k': 'unset!'
  'ctrl-k j': 'unset!'
  'ctrl-k h': 'unset!'

'.platform-darwin .tree-view':
# ====== UNDO ALL THE cmd-K STUFF ======
  'cmd-k up': 'unset!'
  'cmd-k right': 'unset!'
  'cmd-k left': 'unset!'
  'cmd-k down': 'unset!'
  'cmd-k l': 'unset!'
  'cmd-k k': 'unset!'
  'cmd-k j': 'unset!'
  'cmd-k h': 'unset!'

'.platform-win32 .editor, .platform-linux .editor':
    'alt-g': 'go-to-line:toggle'
    'ctrl-a': 'editor:move-to-first-character-of-line'
    'ctrl-e': 'editor:move-to-end-of-line'
    'ctrl-k': 'custom:kill-line'

    'ctrl-backspace': 'editor:delete-to-beginning-of-subword'
    'ctrl-delete': 'editor:delete-to-end-of-subword'

    'ctrl-right': 'editor:move-to-next-subword-boundary'
    'ctrl-left': 'editor:move-to-previous-subword-boundary'
    'ctrl-shift-right': 'editor:select-to-next-subword-boundary'
    'ctrl-shift-left': 'editor:select-to-previous-subword-boundary'


'.platform-darwin .editor':
    'cmd-g':'go-to-line:toggle'
    'cmd-a': 'editor:move-to-first-character-of-line'
    'cmd-e': 'editor:move-to-end-of-line'
    'cmd-k': 'custom:kill-line'

    'ctrl-backspace': 'editor:delete-to-beginning-of-subword'
    'cmd-backspace': 'editor:delete-to-end-of-subword'

    'cmd-right': 'editor:move-to-next-subword-boundary'
    'cmd-left': 'editor:move-to-previous-subword-boundary'
    'cmd-shift-right': 'editor:select-to-next-subword-boundary'
    'cmd-shift-left': 'editor:select-to-previous-subword-boundary'


