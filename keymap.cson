'.platform-win32 body, .platform-linux body':
    # ====== UNDO ALL THE CTRL-K STUFF ======
    'ctrl-k ctrl-u': 'unset!'
    'ctrl-k down': 'unset!'
    'ctrl-k left': 'unset!'
    'ctrl-k right': 'unset!'
    'ctrl-k up': 'unset!'
    'ctrl-k ctrl-w': 'unset!'#'pane:close'
    'ctrl-k ctrl-alt-w': 'unset!'
    'ctrl-k ctrl-up': 'unset!'
    'ctrl-k ctrl-right': 'unset!'
    'ctrl-k ctrl-left': 'unset!'
    'ctrl-k ctrl-down': 'unset!'
    'ctrl-k ctrl-p': 'unset!' #'window:focus-previous-pane'
    'ctrl-k ctrl-n': 'unset!' #'window:focus-next-pane'

    'ctrl-u up': 'pane:split-up-and-copy-active-item'
    'ctrl-u right': 'pane:split-right-and-copy-active-item'
    'ctrl-u left': 'pane:split-left-and-copy-active-item'
    'ctrl-u down': 'pane:split-down-and-copy-active-item'

    'ctrl-u ctrl-up': 'window:focus-pane-above'
    'ctrl-u ctrl-right': 'window:focus-pane-on-right'
    'ctrl-u ctrl-left': 'window:focus-pane-on-left'
    'ctrl-u ctrl-down': 'window:focus-pane-below'
    'alt-o': 'window:focus-next-pane'

    'alt-shift-a':'application:add-project-folder'


'.platform-darwin body':
# ====== UNDO ALL THE cmd-K STUFF ======
  'cmd-k cmd-u': 'unset!'
  'cmd-k down': 'unset!'
  'cmd-k left': 'unset!'
  'cmd-k right': 'unset!'
  'cmd-k up': 'unset!'
  'cmd-k cmd-w': 'unset!'#'pane:close'
  'cmd-k cmd-alt-w': 'unset!'
  'cmd-k cmd-up': 'unset!'
  'cmd-k cmd-right': 'unset!'
  'cmd-k cmd-left': 'unset!'
  'cmd-k cmd-down': 'unset!'
  'cmd-k cmd-p': 'unset!' #'window:focus-previous-pane'
  'cmd-k cmd-n': 'unset!' #'window:focus-next-pane'

  'cmd-shift-T': 'unset!' #'pane:reopen-closed-item'
  'cmd-up':'unset!'
  'cmd-down':'unset!'
  'cmd-`': 'core:close'
  'shift-cmd-T': 'unset!' #'pane:reopen-closed-item'

  'ctrl-A':'application:add-project-folder'

'body':
  'cmd-w': 'core:close'

'.platform-win32 .workspace, .platform-linux .workspace':
  # ==== CURSOR ====
  'alt->': 'core:move-to-bottom'
  'alt-<': 'core:move-to-top'
  'ctrl-shift-e': 'editor:select-to-end-of-line'
  'ctrl-shift-a': 'editor:select-to-beginning-of-line'
  # 'ctrl-x h': 'core:select-all'
  'ctrl-h': 'core:select-all'

  # ==== TEXT MANIPULATION ====
  'ctrl-_': 'core:undo'
  'ctrl-w': 'core:cut'
  'alt-w': 'core:copy'
  'ctrl-y': 'core:paste'

  # ==== BUFFER ====
  'ctrl-g': 'core:cancel'
  'ctrl-`': 'core:close'

'.platform-darwin .workspace':
  # ==== CURSOR ====
  'cmd->': 'core:move-to-bottom'
  'cmd-<': 'core:move-to-top'
  'cmd-shift-e': 'editor:select-to-end-of-line'
  'cmd-shift-a': 'editor:select-to-beginning-of-line'
  'cmd-h': 'core:select-all'

  # ==== TEXT MANIPULATION ====
  # 'ctrl-_': 'core:undo'
  'ctrl-w': 'core:cut'
  'cmd-w': 'core:copy'
  'cmd-y': 'core:paste'

  # ==== BUFFER ====
  'ctrl-g': 'core:cancel'
  'ctrl-`': 'core:close'


'.platform-win32 atom-text-editor, .platform-linux atom-text-editor':
    'ctrl-k ctrl-d': 'unset!' #'find-and-replace:select-skip'

    'alt-right': 'editor:move-to-end-of-screen-line'
    'alt-left': 'editor:move-to-first-character-of-line'
    'ctrl-i': 'editor:auto-indent'

'.platform-darwin atom-text-editor':
    'cmd-k cmd-d': 'unset!' #'find-and-replace:select-skip'

    'cmd-t':'unset!'
    'ctrl-t':'unset!'
    'cmd-g':'unset!'
    'cmd-a':'unset!'
    'cmd-right':'unset!'
    'cmd-left':'unset!'
    'cmd-k cmd-d': 'unset!' #'find-and-replace:select-skip'
    'cmd-delete': 'unset!'#'editor:delete-to-end-of-line'
    'ctrl-A':'unset!'

    'cmd-i': 'editor:auto-indent'

'atom-text-editor:not([mini])':
    'alt-b': 'git-blame:toggle'

'atom-workspace':
    'alt-shift-p': 'plscript:toggle'
    'ctrl-b': 'plscript:add-breakpoint'
    'ctrl-alt-c': 'plscript:clear-breakpoints'

'.platform-win32 atom-workspace atom-text-editor, .platform-win32 atom-workspace atom-text-editor':
    'ctrl-k ctrl-l': 'unset!' #'editor:lower-case'
    'ctrl-k ctrl-u': 'unset!'
    'ctrl-u ctrl-l': 'editor:lower-case'
    'ctrl-u ctrl-u': 'editor:upper-case'

'.platform-darwin atom-workspace atom-text-editor':
    'cmd-k cmd-l': 'unset!'
    'cmd-k cmd-u': 'unset!'
    'cmd-u cmd-l': 'editor:lower-case'
    'cmd-u cmd-u': 'editor:upper-case'

